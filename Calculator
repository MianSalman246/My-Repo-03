using System;

class Program
{
    static void Main()
    {
        string name = "Mian Salman";
        string rollNumber = "24-ST-071";

        Console.WriteLine("Name:");
        for (int i = 0; i < name.Length; i++)
        {
            Console.WriteLine($"Letter {i + 1}: {name[i]}");
        }

        Console.WriteLine("\nRoll Number:");
        for (int i = 0; i < rollNumber.Length; i++)
        {
            Console.WriteLine($"Digit {i + 1}: {rollNumber[i]}");
        }
    }
}
