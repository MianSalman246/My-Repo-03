        int a = 5;  
        int b = 7;  
        Division

        if (b != 0)  
        {
            double division = (double)a / b;
            Console.WriteLine($"The division of {a} by {b} is: {division}");
        }
        else
        {
            Console.WriteLine("Division by zero is not allowed.");
        }
    
        int product = a * b;
        Console.WriteLine($"The multiplication of {a} and {b} is: {product}");
        Multiplication
    }
